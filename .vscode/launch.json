{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:9000",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Attach to Chrome",
        "port": 9222,
        "request": "attach",
        "type": "chrome",
        "webRoot": "${workspaceFolder}"
      },
      {
          "type": "java",
          "name": "Current File",
          "request": "launch",
          "mainClass": "${file}"
      },
      {
          "type": "java",
          "name": "GameDirectoryApp",
          "request": "launch",
          "mainClass": "com.timoleon.gamedirectory.GameDirectoryApp",
          "projectName": "game-directory"
      }
    ]
}
